# fstab for Narzo 30A (RMX3171)
/dev/block/by-name/system     /system     ext4    ro,barrier=1                        wait
/dev/block/by-name/vendor     /vendor     ext4    ro,barrier=1                        wait
/dev/block/by-name/product    /product    ext4    ro,barrier=1                        wait
/dev/block/by-name/data       /data       f2fs    noatime,nosuid,nodev,discard,inlinecrypt,fileencryption=software   wait,check,formattable,resize
/dev/block/by-name/cache      /cache      ext4    nosuid,nodev,barrier=1              wait,check
